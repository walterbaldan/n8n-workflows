    {
      "parameters": {
        "jsCode": "return items.map(item => {\n  const newItem = { ...item }; // Clonamos el item original\n\n  // Verificamos que exista la key \"title\"\n  if (newItem.json.title) {\n    newItem.json.title = 'WalterFundation:  ' + newItem.json.title;\n  }\n\n  return newItem;\n});\n"
      },
